<div class="container-fluid" style="height: 75vh;">
  <div class="d-flex justify-content-center align-items-center h-100">
    <div class="text-center">
      <% @votes.each do |vote| %>
        <p><%= vote.voter.nickname %> voted for <%= vote.voted_player.nickname %></p>
      <% end %>

      <button class="btn btn-primary">Future End Game button</button>
    </div>
  </div>
</div>
